{
  "name": "vue-mxp",
  "version": "1.1.7",
  "description": "",
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.ts",
    "style": "tailwindcss build src/assets/index.css -o src/assets/tailwind.css",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js && npx tailwindcss build src/assets/tailwind.css -o src/assets/index.css",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "main": "dist/vue-mxp.ssr.js",
  "module": "dist/vue-mxp.esm.js",
  "browser": "dist/vue-mxp.esm.js",
  "unpkg": "dist/vue-mxp.min.js",
  "files": [
    "dist/*",
    "vue-mxp.d.ts",
    "src/**/*.vue",
    "src/assets/*.css",
    "postcss.config.js",
    "tailwind.config.js"
  ],
  "dependencies": {
    "@tailwindcss/postcss7-compat": "^2.1.2",
    "autoprefixer": "^9.8.6",
    "postcss": "^7.0.35",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-replace": "^2.3.4",
    "@vue/cli-plugin-babel": "^4.5.10",
    "@vue/cli-plugin-typescript": "^4.5.10",
    "@vue/cli-service": "^4.5.10",
    "@vue/compiler-sfc": "^3.0.5",
    "cross-env": "^7.0.3",
    "minimist": "^1.2.5",
    "rollup": "^2.36.1",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "typescript": "^3.8.3",
    "vue": "^3.0.5",
    "vue-cli-plugin-tailwind": "~2.0.6"
  },
  "peerDependencies": {
    "vue": "^3.0.5"
  },
  "engines": {
    "node": ">=12"
  },
  "sideEffects": false,
  "types": "vue-mxp.d.ts"
}
